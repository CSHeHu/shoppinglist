<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>


    <div id="SLContainer"
    <% if (data && data.length > 0){ %>
      <ul>
        <% data.forEach(item => { %>
          <li><%=item.name%> - <%=item.amount%></li>
        <% }); %>
      </ul>
    <% } %> 
    </div>

    <div id=addToListContainer>
      <form id="addToListForm">
        <input type="text" id="addToListInput" placeholder="AddNewItem" required>
        <button type="submit" id="addToListSubmit">Add</button>
      </form>
    </div>

    

  </body>
</html>
